{
    "manifest_version": 2,
    "name": "Phishing Link Detector",
    "version": "1.0",
    "description": "Automatically prevents you from proceeding upon clicking a malicious link!",
    "permissions": [
        "activeTab",
        "storage",
        "tabs",
        "webRequest",
        "webRequestBlocking"
    ],
    "background": {
        "scripts": ["extension.js"],
        "persistent": false
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["extension.js"]
        }
    ],
    "icons": {
        "16": " ",
        "48": " ",
        "128": " "
    },
    "page_action": {
        "default_icon": {
            "16": " ",
            "48": " ",
            "128": " "
        }
    },
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    }
}
